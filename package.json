{
  "name": "gusfune",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "NODE_ENV=production blitz build && blitz prisma migrate deploy --preview-feature",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@headlessui/react": "0.3.1",
    "@prisma/client": "2.19.0",
    "blitz": "0.33.0",
    "next-seo": "4.23.0",
    "prisma": "2.19.0",
    "react": "0.0.0-experimental-3310209d0",
    "react-content-loader": "6.0.2",
    "react-cookie-consent": "6.2.3",
    "react-dom": "0.0.0-experimental-3310209d0",
    "react-error-boundary": "3.1.1",
    "react-hook-form": "6.15.5",
    "store2": "2.12.0",
    "tailwindcss": "2",
    "typeface-open-sans": "1.1.13",
    "typescript": "~4.1",
    "zod": "1.11.13"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.3",
    "autoprefixer": "10",
    "eslint": "7.22.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "nodemailer": "6.5.0",
    "postcss": "8",
    "prettier": "2.2.1",
    "prettier-plugin-prisma": "0.6.0",
    "pretty-quick": "3.1.0",
    "preview-email": "3.0.4",
    "react-countup": "4.3.3"
  },
  "private": true
}
